---
import { Button } from "@/components/ui/button";

const buttonLabels = ["Portfolio", "Roasting HP", "Keluh Kesah"];
---

<main
  id="container"
  class="mx-auto justify-center items-center flex max-w-screen-sm max-h-screen"
>
  <div class="flex flex-col space-y-2">
    {buttonLabels.map((label) => (
	  <Button>{label}</Button>
	))}
  </div>
</main>
